FROM quay.io/mouricedevs/gifted-md
RUN git clone https://github.com/mouricedevs/Gifted-Md /root/Gifted
WORKDIR /root/Gifted
RUN npm install
EXPOSE 8000
CMD ["npm","start" ] 


































































































































































































